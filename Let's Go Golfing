local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "Let's Go Golfing",
    SubTitle = "by Elton",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, 
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})

--tabs
local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "power" }),
}

Tabs.Main:AddButton({
    Title = "Move the Ball into the Hole",
    Description = "Risk of getting banned",
    Callback = function()
        local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
local lp = Players.LocalPlayer

local ball
for _, obj in pairs(workspace:FindFirstChild("Balls"):GetChildren()) do
    if obj.Name:find(lp.Name) and obj:FindFirstChild("BallPart") then
        ball = obj:FindFirstChild("BallPart")
        break
    end
end

local mapFolder = workspace:FindFirstChild("LoadedMap")
local currentMap = mapFolder and mapFolder:FindFirstChildWhichIsA("Folder")
local mainTiles = currentMap and currentMap:FindFirstChild("Main Tiles")
local hole = mainTiles and mainTiles:FindFirstChild("Hole")

local holeEnd = hole and hole:FindFirstChild("End")
local flag = hole and hole:FindFirstChild("Flag")
local flagEnd = flag and flag:FindFirstChild("End")

local function tweenBallTo(part)
    if not (ball and part and ball:IsA("BasePart")) then
        return
    end

    local goal = {}
    goal.Position = part.Position + Vector3.new(0, 0.5, 0)

    local tweenInfo = TweenInfo.new(
        1.5,
        Enum.EasingStyle.Linear,
        Enum.EasingDirection.Out
    )

    local tween = TweenService:Create(ball, tweenInfo, goal)
    tween:Play()
end

if ball then
    tweenBallTo(holeEnd or flagEnd)
end

    end
})
